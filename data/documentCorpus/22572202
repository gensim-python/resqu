                        jQuery(document).ready( function () {
                            jQuery("#send_to_menu input[type='radio']").click( function () {
                                var selectedValue = jQuery(this).val().toLowerCase();
                                var selectedDiv = jQuery("#send_to_menu div." + selectedValue);
                                
                                if(selectedDiv.is(":hidden")){ 
                                jQuery("#send_to_menu div.submenu:visible").slideUp();
                                selectedDiv.slideDown();
                                }
                            });
                        
                        });
                        
                        jQuery("#sendto").bind("ncbipopperclose", function(){
                            jQuery("#send_to_menu div.submenu:visible").css("display","none");
                            jQuery("#send_to_menu input[type='radio']:checked").attr("checked",false);
                        });
                        
                    </script></h4><div id="send_to_menu" class="tabPopper send_to"><fieldset><legend>Choose Destination</legend><ul class="column_list"><li><input type="radio" name="EntrezSystem2.PEntrez.PubMed.Pubmed_ResultsPanel.Pubmed_DisplayBar.SendTo" sid="1" value="File" id="dest_File" /><label for="dest_File">File</label></li><li><input type="radio" name="EntrezSystem2.PEntrez.PubMed.Pubmed_ResultsPanel.Pubmed_DisplayBar.SendTo" sid="2" value="AddToClipboard" id="dest_AddToClipboard" /><label for="dest_AddToClipboard">Clipboard</label></li><li><input type="radio" name="EntrezSystem2.PEntrez.PubMed.Pubmed_ResultsPanel.Pubmed_DisplayBar.SendTo" sid="3" value="AddToCollections" id="dest_AddToCollections" /><label for="dest_AddToCollections">Collections</label></li><li><input type="radio" name="EntrezSystem2.PEntrez.PubMed.Pubmed_ResultsPanel.Pubmed_DisplayBar.SendTo" sid="4" value="Mail" id="dest_Mail" /><label for="dest_Mail">E-mail</label></li><li><input type="radio" name="EntrezSystem2.PEntrez.PubMed.Pubmed_ResultsPanel.Pubmed_DisplayBar.SendTo" sid="5" value="Order" id="dest_Order" /><label for="dest_Order">Order</label></li><li><input type="radio" name="EntrezSystem2.PEntrez.PubMed.Pubmed_ResultsPanel.Pubmed_DisplayBar.SendTo" sid="6" value="AddToBibliography" id="dest_AddToBibliography" /><label for="dest_AddToBibliography">My Bibliography</label></li><li><input type="radio" name="EntrezSystem2.PEntrez.PubMed.Pubmed_ResultsPanel.Pubmed_DisplayBar.SendTo" sid="7" value="CitationManager" id="dest_CitationManager" /><label for="dest_CitationManager">Citation manager</label></li></ul></fieldset><div class="submenu file" id="submenu_File" style="display: none;"><p id="submenu_File_hint" class="hidden"></p><ul><li><label for="file_format">Format</label><select id="file_format" name="EntrezSystem2.PEntrez.PubMed.Pubmed_ResultsPanel.Pubmed_DisplayBar.FFormat" sid="1"><option value="docsum" format="text">Summary (text)</option><option value="abstract" format="text"